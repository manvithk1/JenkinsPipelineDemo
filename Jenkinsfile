pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World1111'
            }
        }
        stage('Build') {
            steps {
                echo 'Building1111'
                sh 'java -version'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying1111'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing1111'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing1111'
            }
        }
    }
}
